
/* ~/Xresources */

/*#######################################################*/
*.foreground: #eeeeee
! *.background: #041517
*.background: #101010
*.cursorColor: #ff6a6a

! black
*.color0: #000000
*.color8: #657b83

! red
*.color1: #dc322f
*.color9: #dc322f

! green
*.color2: #859900
*.color10: #859900

! yellow
*.color3: #b58900
*.color11: #b58900

! blue
*.color4: #268bd2
*.color12: #268bd2

! magenta
*.color5: #6c71c4
*.color13: #6c71c4

! cyan
*.color6: #2aa198
*.color14: #2aa198

! white
*.color7: #93a1a1
*.color15: #fdf6e3

/*#######################################################*/

! Xft.dpi: 142
! Xft.autohint: false
! Xft.antialias: true
Xcursor.size: 16
*.font: Literation Mono Nerd Font:pixelsize=12:autohint=false:antialias=true

/*#######################################################*/

! string
*.normfgcolor: #eeeeee
*.normbgcolor: #101010
*.selfgcolor: #268bd2
*.selbgcolor: #202020

/*#######################################################*/
! string
dwm.normbordercolor: #303030
dwm.selbordercolor: #268bd2
dwm.selspcbordercolor: #b58900
dwm.normspcbordercolor: #050505

! Float
dwm.mfact: 0.50

! Integer
dwm.borderpx:1
dwm.snap: 10 
dwm.showbar: 1
dwm.topbar: 1
dwm.nmaster: 1
dwm.resizehints: 0
dwm.showsystray: 0
dwm.lockfullscreen: 1
dwm.focusonwheel: 0
dwm.systraypinning: 0
dwm.systrayspacing: 2
dwm.systrayonleft: 0
dwm.systraypinningfailfirst: 1
dwm.showtitle: 1

/*#######################################################*/
! string
! st.font: Literation Mono:style=Regular:pixelsize=10
! st.termname: st-256color
! st.shell: "/bin/sh"

! float
! st.cwscale: 1.1
! st.chscale: 1.0
st.alpha: 0.98

! integer
! st.minlatency: 8
! st.maxlatency: 33
! st.blinktimeout: 800
! st.bellvolume: 0
! st.tabspaces: 8
! st.borderpx: 0

/*#######################################################*/
! string
dmenu.font: Literation Mono Nerd Font:pixelsize=10:autohint=false:antialias=true
