/*#######################################################*/

! string
*.normfg: #ffffff
*.normbg: #101010
*.selfg: #43e954
*.selbg: #202020

dwm.statusbg: #282036
! dwm.normbg: #101010
! dwm.selbg: #202020
! dwm.selbg: #282036
! dwm.normbg: #282036
dwm.selbg: #192617
dwm.normbg: #192617
dwm.gaps: 2

dmenu.normfg: #eeeeee
! dmenu.normbg: #282036
dmenu.normbg: #192617
dmenu.selfg: #000000
dmenu.selbg: #43e954

/*#######################################################*/
! string
dwm.normborder: #777777
dwm.selborder: #43e954
dwm.normstickyborder: #050505
dwm.selstickyborder: #dc322f
dwm.normloatborder: #050505
dwm.selfloatborder: #b58900

dwm.statusfg_1: #43e954
dwm.statusfg_2: #43e944
dwm.statusfg_3: #43e944
dwm.statusfg_4: #43e954
dwm.statusfg_5: #43e954

! Float
dwm.mfact: 0.50

! Integer
dwm.borderpx: 3
dwm.snap: 10
dwm.systrayspacing: 2

! Boolean
dwm.showbar: 1
dwm.topbar: 1
dwm.nmaster: 1
dwm.resizehints: 0
dwm.showsystray: 1
dwm.lockfullscreen: 1
dwm.focusonwheel: 0
dwm.systraypinning: 0
dwm.systrayonleft: 0
dwm.systraypinningfailfirst: 1
dwm.showtitle: 1
dwm.block_inversed : 1
dwm.status_color_inversed : 0
! dwm.block_delimiter : |

/*#######################################################*/
! string
! st.font: Literation Mono:style=Regular:pixelsize=10
! st.termname: st-256color
! st.shell: "/bin/sh"

! float
! st.cwscale: 1.01
! st.chscale: 1.00
st.alpha: 0.90

/*
 * 0: blinking block
 * 1: blinking block (default)
 * 2: steady block ("â–ˆ")
 * 3: blinking underline
 * 4: steady underline ("_")
 * 5: blinking bar
 * 6: steady bar ("|")
 * 7: blinking st cursor
 * 8: steady st cursor
 */

st.cursorStyle : 2
st.cursorThickness : 4
st.brightBold : 0

! integer
! st.minlatency: 8
! st.maxlatency: 33
! st.blinktimeout: 800
! st.bellvolume: 0
! st.tabspaces: 8
st.borderpx: 2

xscreensaver.splash: false

!font settings
! xscreensaver.Dialog.headingFont:        -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.Dialog.bodyFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.Dialog.labelFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.Dialog.unameFont:          -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.Dialog.buttonFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.Dialog.dateFont:           -*-dina-medium-r-*-*-12-*-*-*-*-*-*-*
! xscreensaver.passwd.passwdFont:         -*-dina-bold-r-*-*-12-*-*-*-*-*-*-*
!

!general dialog box (affects main hostname, username, password text)
xscreensaver.Dialog.foreground:       #43e954
xscreensaver.Dialog.background:         #192617
xscreensaver.Dialog.topShadowColor:     #192617
xscreensaver.Dialog.bottomShadowColor:  #192617
xscreensaver.Dialog.Button.foreground:  #43e954
xscreensaver.Dialog.Button.background:  #192617

!username/password input box and date text colour
xscreensaver.Dialog.text.foreground:    #43e954
xscreensaver.Dialog.text.background:    #192617
xscreensaver.Dialog.internalBorderWidth:24
xscreensaver.Dialog.borderWidth:        20
xscreensaver.Dialog.shadowThickness:    2

!timeout bar (background is actually determined by Dialog.text.background)
xscreensaver.passwd.thermometer.foreground:  #ff0000
xscreensaver.passwd.thermometer.background:  #000000
xscreensaver.passwd.thermometer.width:       8

!datestamp format--see the strftime(3) manual page for details
xscreensaver.dateFormat:    %I:%M%P %b %d
